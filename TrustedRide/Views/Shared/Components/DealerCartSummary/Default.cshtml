@model DealerCartVM

@if (Model.DealerCart.DealerCartItems.ToList().Count > 0)
{
    <li>
        <a>
            <span class="glyphicon glyphicon-shopping-cart"></span>
            <span id="cart-status">
                @Model.DealerCart.DealerCartItems.ToList().Count
            </span>
        </a>
    </li>
}